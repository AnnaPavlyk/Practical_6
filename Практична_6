#include <stdio.h>
#include <math.h>

int main() {
    int x1, y1, x2, y2;
    double length;

    // Введення координат в одному рядку
    printf("Координати початку та кінця вектора (x1 y1 x2 y2): ");
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);

    length = sqrt(pow(x2 - x1, 2) + pow(y2 - y1, 2));   // Обчислення довжини вектора

    printf("Довжина вектора: %.6lf\n", length);

    return 0;
}
